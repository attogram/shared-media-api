{
    "name": "attogram/shared-media-api",
    "description": "MediaWiki Query API wrapper to easily get Category and Media file information into simple PHP arrays. Fine-tuned for WikiMedia Commmons.",
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/attogram/shared-media-api",
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.5 || ^7.0",
        "guzzlehttp/guzzle": "^6.2@dev",
        "psr/log": "~1.0"
    },
    "require-dev": {
        "attogram/shared-media-sandbox": "^1.1.2@dev"
    },
    "autoload": {
        "psr-4": {
            "Attogram\\SharedMedia\\Api\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
	"suggest": {
		"attogram/shared-media-orm": "ORM for attogram/shared-media-api"
	}
}
